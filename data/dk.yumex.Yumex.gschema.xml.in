<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="yumex">
	<schema id="@APP_ID@" path="/dk/yumex/Yumex/">
		<key name="window-width" type="i">
			<default>1000</default>
			<summary>last window width</summary>
		</key>
		<key name="window-height" type="i">
			<default>700</default>
			<summary>last window height</summary>
		</key>
		<key name="window-maximized" type="b">
			<default>false</default>
			<summary>window maximized</summary>
		</key>
		<key name="window-fullscreen" type="b">
			<default>false</default>
			<summary>window fullscreen</summary>
		</key>
		<key name="col-name-width" type="i">
			<default>200</default>
			<summary>width of name column</summary>
		</key>
		<key name="col-version-width" type="i">
			<default>100</default>
			<summary>width of version column</summary>
		</key>
		<key name="col-repo-width" type="i">
			<default>100</default>
			<summary>width of repo column</summary>
		</key>
		<key name="col-size-width" type="i">
			<default>100</default>
			<summary>width of size column</summary>
		</key>
		<key name="col-arch-width" type="i">
			<default>100</default>
			<summary>width of arch column</summary>
		</key>
		<key name="col-description-width" type="i">
			<default>250</default>
			<summary>width of summary column</summary>
		</key>
		<key name="pkg-paned-pos" type="i">
			<default>900</default>
			<summary>position of the divider for the buttom panel</summary>
		</key>
		<key name="fp-location" type="s">
			<default>"user"</default>
			<summary>default flatpak install location</summary>
		</key>
		<key name="fp-remote" type="s">
			<default>"flathub"</default>
			<summary>default flatpak remote</summary>
		</key>
		<key name="meta-load-time" type="x">
			<default>0</default>
			<summary>last metadata refresh time</summary>
		</key>
		<key name="meta-load-periode" type="i">
			<default>3600</default>
			<summary>minimum time betwwen metadata refresh (seconds( </summary>
		</key>
		<key name="upd-custom" type="s">
			<default>""</default>
			<summary>path to custom updater in systray</summary>
		</key>
		<key name="upd-show-icon" type="b">
			<default>true</default>
			<summary>show systray icon on updates</summary>
		</key>
		<key name="upd-interval" type="i">
			<default>3600</default>
			<summary>time frequency for check for new updates in the background</summary>
		</key>
		<key name="upd-notification" type="b">
			<default>true</default>
			<summary>Send notification on updates</summary>
		</key>
		<key name="upd-dark-icon" type="b">
			<default>false</default>
			<summary>Use a dark updaer icon</summary>
		</key>
	</schema>
</schemalist>
